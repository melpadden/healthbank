    "@angular/router": "^5.2.5",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@zxing/ngx-scanner": "^1.2.0",
    "bootstrap": "^4.0.0-beta.3",
    "bs58": "^4.0.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "file-saver": "^1.3.8",
    "jquery": "^3.3.1",
    "jspdf": "^1.4.1",
    "jwt-decode": "2.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.18.1",
    "msgpack-lite": "^0.1.26",
    "ng2-material-dropdown": "^0.9.5",
    "ng2-pdf-viewer": "^5.1.1",
    "ngx-breadcrumbs": "0.0.3",
