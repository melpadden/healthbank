{
  "name": "mediplan-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check --extract-css --proxy-config serve-proxy.conf.json --base-href /ui/ --ssl true",
    "build": "ng build --prod --aot --base-href /",
    "test": "ng test --progress=false --single-run --code-coverage",
    "ts-lint": "ng lint --type-check",
    "compress": "node scripts/compress.js",
    "lint": "concurrently -r \"npm run ts-lint -- --fix\" \"npm run stylelint\"",
    "lint-ci": "mkdirp \"target/lint\" && concurrently -r \"ng lint --type-check --format checkstyle > target/lint/tslint.xml\" \"stylelint --syntax scss \"src/**/*.scss\" --custom-formatter node_modules/stylelint-checkstyle-formatter/index.js > target/lint/stylelint.xml\" || ((>&2 echo \"There are front-end lint errors! Check for errors in target/lint/\") && exit 1 )",
    "stylelint": "stylelint --syntax scss \"src/**/*.scss\"",
    "commit-check": "npm run test && ng lint --fix && npm run stylelint",
    "e2e": "ng e2e --proxy-config serve-proxy.conf.json --base-href /ui/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
